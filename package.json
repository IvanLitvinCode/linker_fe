{
  "name": "needl",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-storybook": "build-storybook",
    "build:prod": "tsc --noEmit && vite build --mode prod",
    "build:demo": "tsc --noEmit && vite build --mode demo",
    "build:qa1": "tsc --noEmit && vite build --mode qa --sourcemap true",
    "build:setup": "tsc --noEmit && vite build --mode prod",
    "build:stage": "tsc --noEmit && vite build --mode stage --sourcemap true",
    "build:stage-ml": "tsc --noEmit && vite build --mode stage-ml --sourcemap true",
    "build:stage-app-1": "tsc --noEmit && vite build --mode stage-app-1 --sourcemap true",
    "build:stage-app-2": "tsc --noEmit && vite build --mode stage-app-2 --sourcemap true",
    "build:stage-app-3": "tsc --noEmit && vite build --mode stage-app-3 --sourcemap true",
    "build:stage-devops": "tsc --noEmit && vite build --mode stage-devops --sourcemap true",
    "build:test": "tsc --noEmit && vite build --mode prod",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --cache --cache-location ./node_modules/.cache/eslint . --fix",
    "serve": "vite preview",
    "setup": "yarn install --frozen-lockfile && CI=true yarn test && yarn build:test",
    "start": "vite --mode dev",
    "storybook": "start-storybook -p 6006",
    "test": "./test.sh",
    "test:ci": "vitest run --silent",
    "test:local": "vitest watch --ui --changed",
    "typecheck": "tsc --noEmit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
